{
  "name": "@lksnext/ai-core-tools-base",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build --mode library && tsc -p tsconfig.build.json",
    "build:lib:watch": "tsc -p tsconfig.build.json --watch & vite build --mode library --watch",
    "build:production": "tsc -b --noCheck && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "pack:local": "npm run build:lib && npm pack",
    "prepare": "npm run build:lib"
  },
  "dependencies": {
    "@types/node": "^24.1.0",
    "axios": "^1.10.0",
    "oidc-client-ts": "^3.3.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.30.1",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^4.5.3"
  }
}
